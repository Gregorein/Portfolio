{
  "name": "gregbak",
  "description": "greg bak portfolio",
  "version": "0.2.0",
  "private": true,
  "license": "All rights reserved.",
  "main": "src/index.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && yarn run -s serve || yarn run -s dev",
    "build": "preact build --no-prerender --template src/template.html",
    "serve": "preact build --no-prerender --template src/template.html && preact serve",
    "dev": "preact watch --template src/template.html",
    "api": "http-server -p 8000 --cors"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "classnames": "^2.2.6",
    "eslint": "^7.6.0",
    "http-server": "^0.12.3",
    "identity-obj-proxy": "^3.0.0",
    "if-env": "^1.0.4",
    "node-sass": "^4.14.1",
    "preact-cli": "^3.0.1",
    "preact-context": "^1.1.4",
    "preact-render-spy": "^1.3.0",
    "react-redux": "^7.2.1",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "sass-loader": "^9.0.2",
    "webpack-bundle-analyzer": "^3.8.0"
  },
  "dependencies": {
    "moment": "^2.27.0",
    "preact": "^10.4.6",
    "preact-router": "^3.2.1",
    "smooth-scroll": "^16.1.3"
  }
}
